	S CNT=0 
	K ^DADDX("DDREF")
	S DDNM=""
L1	S DDNM=$O(^DADD(DDNM)) I DDNM="" G L0
	S DDREF=$P(^DADD(DDNM,"REF"),";",1)
	I DDREF]"" S %EXT=DDREF DO XREF^%DASDUTL S:%INT'="" ^DADDX("DDREF",%INT,DDNM)=%EXT
